  "license": "MIT",
  "description": "LibrerÃ­a de react para reportar errores en aplicaciones React",
  "main": "./dist/cf-react-error-reporter.umd.js",
  "module": "./dist/cf-react-error-reporter.es.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/cf-react-error-reporter.es.js",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cf-react-error-reporter.umd.js"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
